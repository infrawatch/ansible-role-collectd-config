{% if collectd_plugin_dns_interval is not defined %}
LoadPlugin dns
{% else %}
<LoadPlugin dns>
   Interval {{ collectd_plugin_dns_interval }}
</LoadPlugin>
{% endif %}

<Plugin "dns">
{% if collectd_plugin_dns_interface is defined %}
  Interface "{{ collectd_plugin_dns_interface }}"
{% endif %}
{% if collectd_plugin_dns_ignoresource is defined %}
  IgnoreSource "{{ collectd_plugin_dns_ignoresource }}"
{% endif %}
{% if collectd_plugin_dns_selectnumericquerytypes is defined %}
  SelectNumericQueryTypes "{{ collectd_plugin_dns_selectnumericquerytypes }}"
{% endif %}
</Plugin>
